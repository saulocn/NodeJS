<html>

<body>
    <h1>Listando os Produtos</h1>

    <table>
        <thead>
            <th>
                <td>id</td>
                <td>titulo</td>
                <td>descricao</td>
                <td>preco</td>
            </th>
        </thead>
        <tbody>
            <%for(var i=0;i<lista.length;i++){%>
                <tr>
                    <td>
                        <%=lista[i].id%>
                    </td>
                    <td>
                        <%=lista[i].titulo%>
                    </td>
                    <td>
                        <%=lista[i].descricao%>
                    </td>
                    <td>
                        <%=lista[i].preco%>
                    </td>
                </tr>
                <%}%>
        </tbody>
    </table>
</body>

</html>
